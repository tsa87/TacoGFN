    "cond": {
        "temperature": {
            "sample_dist": "uniform",
            "dist_params": [
                0,
                64.0
            ]
        }
    },
    "task": {
        "pharmaco_frag": {
            "fragment_type": "zinc250k_50cutoff_brics",
            "affinity_predictor": "alpha",
            "leaky_coefficient": 0.2,
            "reward_multiplier": 3.0,
            "max_qed_reward": 0.7,
            "max_sa_reward": 0.8,
            "objectives": [
                "docking",
                "qed",
                "sa"
            ]
        }
    },
    "model": {
        "pharmaco_cond": {
            "pharmaco_dim": 128
        },
        "num_emb": 256,
        "num_layers": 2
    }
}